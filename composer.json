{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "astrotomic/laravel-translatable": "^11.14",
        "darryldecode/cart": "^4.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^11.0",
        "laravel/passport": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "milon/barcode": "^11.0",
        "mpdf/mpdf": "^8.0",
        "nunomaduro/collision": "^8.0",
        "paypal/paypal-checkout-sdk": "^1.0",
        "silber/bouncer": "^1.0",
        "soved/laravel-gdpr": "^1.5",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-newsletter": "^5.0",
        "webpatser/laravel-uuid": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^11.3",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/string.php",
            "app/Helpers/number.php",
            "app/Helpers/slugify.php",
            "app/Helpers/weight.php",
            "app/Helpers/filters.php",
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "arcesilas/active-state",
                "laravel/telescope"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "symfony/thanks": true
        }
    }
}
