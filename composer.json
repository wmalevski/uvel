{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "albertcht/invisible-recaptcha": "^1.8",
        "arcesilas/active-state": "2.0",
        "darryldecode/cart": "~3.0",
        "davejamesmiller/laravel-breadcrumbs": "4.x",
        "dimsav/laravel-translatable": "8.1",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "milon/barcode": "^6.0",
        "mpdf/mpdf": "^8.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "silber/bouncer": "v1.0.0-rc.1",
        "soved/laravel-gdpr": "^1.5",
        "spatie/laravel-newsletter": "^4.2",
        "webpatser/laravel-uuid": "3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/string.php",
            "app/Helpers/number.php",
            "app/Helpers/slugify.php",
            "app/Helpers/weight.php",
            "app/Helpers/filters.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "arcesilas/active-state"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "symfony/thanks": true
        }
    }
}
